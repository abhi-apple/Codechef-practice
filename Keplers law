#include<iostream>
#include<cmath>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int t1;
        int t2;
        int r1;
        int r2;
        cin>>t1>>t2>>r1>>r2;
        float rat1=pow(t1,2)/pow(r1,3);
        float rat2=pow(t2,2)/pow(r2,3);
        cout<<rat1<<endl;
        if(rat1==rat2){
            cout<<"YES"<<endl;
        }else{
            cout<<"NO"<<endl;
        }
    }
}
